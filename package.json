{
  "name": "vite-pug-sass-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "convert-fonts": "node scripts/convert-fonts.js",
    "convert-images": "node scripts/convert-images.js && node scripts/convert-svg.js",
    "convert-assets": "npm run convert-fonts && npm run convert-images",
    "lint": "npm run lint:js && npm run lint:css && npm run lint:pug",
    "lint:js": "eslint src/**/*.js --fix",
    "lint:css": "stylelint src/**/*.{css,scss} --fix",
    "lint:pug": "pug-lint src/pug",
    "format": "prettier --write .",
    "format:js": "prettier --write src/**/*.js",
    "format:css": "prettier --write src/**/*.{css,scss}",
    "format:pug": "prettier --write src/**/*.pug",
    "check-format": "prettier --check .",
    "lint-staged": "npm run lint && npm run format"
  },
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@prettier/plugin-pug": "^3.4.0",
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-inject": "^5.0.5",
    "autoprefixer": "^10.4.16",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.1",
    "postcss": "^8.4.31",
    "prettier": "^3.5.3",
    "pug-lint": "^2.7.0",
    "sass-embedded": "^1.69.0",
    "sharp": "^0.34.2",
    "stylelint": "^16.20.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-standard-scss": "^15.0.1",
    "stylelint-scss": "^6.12.0",
    "svgo": "^3.3.2",
    "terser": "^5.24.0",
    "ttf2woff": "^3.0.0",
    "ttf2woff2": "^4.0.5",
    "vite": "npm:rolldown-vite@latest",
    "vite-plugin-image-optimizer": "^1.1.8",
    "vite-plugin-pug": "^0.4.1"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "dependencies": {
    "normalize.css": "^8.0.1",
    "swiper": "^11.2.8",
    "vanilla-lazyload": "^19.1.3"
  }
}
