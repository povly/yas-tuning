template#h-car__logo-img
  .h-car__logo-img
    img(src="" width="58" height="58" loading="lazy")
template#h-car__list-li
  li.h-car__list-li
    a(href="#choose-your-car").h-car__list-link
template#h-car__data-content
  .h-car__data-content
    table(width="100%" border="0" cellspacing="0" cellpadding="0").h-car__table
      thead
        tr
          th
          th Original
          th After tuning
          th Difference
      tbody
