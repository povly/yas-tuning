      li.h-car__breadcrumb.h-car__breadcrumb_wagens
        a(href="#choose-your-car").h-car__breadcrumb-link Waganes
      li.h-car__breadcrumb.h-car__breadcrumb_brand
        a(href="#choose-your-car").h-car__breadcrumb-link
